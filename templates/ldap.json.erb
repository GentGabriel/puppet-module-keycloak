{
  "id" : "<%= @name %>",
  "name" : "<%= @resource_name %>",
  "providerId" : "ldap",
  "providerType" : "org.keycloak.storage.UserStorageProvider",
  "parentId" : "<%= @realm %>",
  "config" : {
    "pagination" : [ "true" ],
    "debug" : [ "false" ],
    "fullSyncPeriod" : [ "-1" ],
    "searchScope" : [ "1" ],
    "useTruststoreSpi" : [ "<%= @use_truststore_spi %>" ],
    "connectionPooling" : [ "true" ],
    "usersDn" : [ "<%= @user_dn %>" ],
    "cachePolicy" : [ "DEFAULT" ],
    "useKerberosForPasswordAuthentication" : [ "false" ],
    "priority" : [ "<%= @priority %>" ],
    "importEnabled" : [ "<%= @import_enabled %>" ],
<%- if @auth_type == 'simple' -%>
    "bindDn": [""],
    "bindCredential": [""],
<%- end -%>
    "userObjectClasses" : [ "<%= @user_objectclasses.join(',') %>" ],
    "usernameLDAPAttribute" : [ "<%= @username_ldap_attribute %>" ],
    "changedSyncPeriod" : [ "-1" ],
    "rdnLDAPAttribute" : [ "<%= @rdn_ldap_attribute %>" ],
    "vendor" : [ "other" ],
    "editMode" : [ "<%= @edit_mode %>" ],
    "uuidLDAPAttribute" : [ "<%= @uuid_ldap_attribute %>" ],
    "connectionUrl" : [ "<%= @connection_url %>" ],
    "allowKerberosAuthentication" : [ "false" ],
    "syncRegistrations" : [ "false" ],
    "batchSizeForSync" : [ "1000" ],
    "authType" : [ "<%= @auth_type %>" ]
  }
}